// -*- mode: jsonc -*-
[{
    "layer": "top",
    "position": "top",
    "height": 25,
    "spacing": 3,
    "margin-left": 0,
    "margin-right": 0,
    "margin-top": 0,
    "name": "bar1",
    "modules-left": [
        "sway/workspaces",
        "sway/mode",
        "sway/scratchpad"
    ],
    /*"modules-center": [
        "sway/window"
    ],*/
    "modules-right": [
        "pulseaudio",
        "cpu",
        "memory",
        "clock",
        "custom/notification"
    ],
    "sway/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "warp-on-scroll": false,
        "format": "{index}: {name}", // {name}: {icon}
        "format-icons": {
            "1": "ÔÑ†",
            "2": "Ôâ®",
            "3": "ÔÅµ",
            "4": "ÔÜº",
            "5": "üïÆ",
            "urgent": "ÔÅ™",
            "focused": "ÔÜí",
            "default": "ÔÑë"
        }
    },
    "sway/mode": {
        "format": "<span style=\"italic\">{}</span>"
    },
    "sway/scratchpad": {
        "format": "{icon} {count}",
        "show-empty": false,
        "format-icons": ["", "Ôãí"],
        "tooltip": true,
        "tooltip-format": "{app}: {title}"
    },
    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format-alt": "{:%Y-%m-%d}"
    },
    "cpu": {
        "format": "{usage}% Ôãõ",
        "tooltip": false
    },
    "memory": {
        "format": "{}% ÔÉâ"
    },
    "pulseaudio": {
